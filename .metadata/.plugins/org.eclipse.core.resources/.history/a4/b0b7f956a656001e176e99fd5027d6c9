package 연산자test;

import javax.swing.JOptionPane;

public class test01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String pw1 = "p";
		String pw2 = JOptionPane.showInputDialog("암호를 입력해주세요");
		
		if(pw1.equals(pw2)) {
			System.out.println("pass!");
		}else {
			System.out.println("재입력!");
		}
	}

}
