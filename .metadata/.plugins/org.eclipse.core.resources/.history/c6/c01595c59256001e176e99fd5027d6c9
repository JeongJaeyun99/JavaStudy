package 연산자;

public class 산술연산자 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 200;
		int y = 100;
		// 한줄복사 : 컨트를+알트+화살표 아래
		
		System.out.println("두 수의 합은 " + (x + y));
		System.out.println("두 수의 차은 " + (x - y));
		System.out.println("두 수의 곱은 " + (x * y));
		System.out.println("두 수의 나눗셈은 " + (x / y));
		
		
		
	}

}
